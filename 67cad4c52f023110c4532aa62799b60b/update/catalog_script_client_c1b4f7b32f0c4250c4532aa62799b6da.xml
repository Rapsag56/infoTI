        var answer = response.responseXML.documentElement.getAttribute("answer");
        alert(answer);
        var filterString = 'responsavel=' + answer;
        var myListCollector = g_list.get('categoria');
        myListCollector.setQuery(filterString);

    }


}]]></script>

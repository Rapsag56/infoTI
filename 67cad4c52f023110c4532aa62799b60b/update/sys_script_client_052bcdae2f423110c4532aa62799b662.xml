        <script><![CDATA[function onSubmit() {

    var fieldName = 'percentual';


    var fieldValue = g_form.getValue(fieldName);

    if (fieldValue && /[a-zA-Z]/.test(fieldValue)) {
        g_form.showFieldMsg(fieldName, 'Não é permitido inserir letras neste campo.', 'error');
        return false;
    } else {
        var n = fieldValue;

        var nu = n.replace(',', '.') / 100;
       g_form.setValue(fieldName, nu);
    }

    return true;
}]]></script>

        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading) {
    if (isLoading || newValue == '') {
        return;
    }

    var aux = g_form.getDisplayValue('categoria');
	g_form.setValue('categoria_v2', aux);

    // Verifique se o campo 'categoria' foi alterado
    if (oldValue != aux) {
        // Limpe o campo 'subcategoria'
        g_form.setValue('subcategoria', ''); // Define o valor como vazio para limpar
    }

    var filterString = 'categoria.categoria=' + aux;
    var myListCollector = g_list.get('subcategoria');
    myListCollector.setQuery(filterString);
}]]></script>

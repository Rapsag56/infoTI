        <script><![CDATA[var json = JSON.parse(producer.temas);
var gr2 = new GlideRecord("x_744286_informaco_aprova_es_infoti");
gr2.initialize();
gr2.gerente_especialista = producer.gerente_especialista;
gr2.ponto_focal = producer.ponto_focal;
gr2.aprovadores = producer.gerente_aprovador_info_ti;
gr2.solitacao = producer.descreva_sua_solicitacao;
gr2.relatorio_destinatario = producer.relat_rio_destinatario;
gr2.status_da_solicitacao = "aguardando_gerente_infoti";
gr2.categoria = producer.categoria_v2;
var temaValues = [];
for (var n = 0; n < json.length; n++) {
    temaValues.push(json[n].temav2);
}
gr2.tema = temaValues.join(',');
var sysid = gr2.insert();

for(var i = 0; i < json.length; i++){
var gr = new GlideRecord("x_744286_informaco_informacoes_infoti");
gr.initialize();
gr.trimestre = json[i].trimestre;
gr.mensal = json[i].mensal;
gr.periodo_v2 = json[i].periodo_v3;
gr.valor = json[i].valor_v2;
gr.tema = json[i].temav2; 
gr.pontos_de_atencao = json[i].pontos_de_atencao;	
gr.subcategoria = producer.subcategoria;
//gr.status = "2";
gr.numero_da_aprovacao = sysid.toString();
gr.insert();

}


current.setAbortAction(true);]]></script>
